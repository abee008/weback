<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="WeChatToken" value=""/>
	</appSettings>
	<connectionStrings/>
	<system.web>
        <compilation debug="true"></compilation>
        <sessionState mode="InProc" timeout="120"></sessionState>
        <httpRuntime maxRequestLength="20480" executionTimeout="120"></httpRuntime>       
    </system.web>
	<system.webServer>
		<staticContent>
			<mimeMap fileExtension=".ttf" mimeType="application/octet-stream"/>
			<mimeMap fileExtension=".png" mimeType="image/x-png"/>
			<mimeMap fileExtension=".gif" mimeType="image/gif"/>
			<mimeMap fileExtension=".jpg" mimeType="image/jpeg"/>
		</staticContent>
	</system.webServer>
</configuration>
